%YAML 1.1
%TAG !u! tag:unity3d.com,2011:
--- !u!74 &1
MonoBehaviour:
  m_ObjectHideFlags: 0
  m_CorrespondingSourceObject: {fileID: 0}
  m_PrefabInstance: {fileID: 0}
  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 0}
  m_Enabled: 1
  m_AnimatorBindings:
  - path: ""
    type: 2
    property: m_LocalScale.x
    curve:
      serializedVersion: 2
      m_Curve: !!omap
      - 0: {time: 0.0, value: 1.0}   # Start full size
      - 1: {time: 0.2, value: 0.9}   # Shrinking
      - 2: {time: 0.4, value: 0.8}  
      - 3: {time: 0.6, value: 0.7}
      - 4: {time: 0.8, value: 0.6}
      - 5: {time: 1.0, value: 0.5}   # End small
  - path: ""
    type: 2
    property: m_LocalScale.y
    curve:
      serializedVersion: 2
      m_Curve: !!omap
      - 0: {time: 0.0, value: 1.0}
      - 1: {time: 0.2, value: 0.9}
      - 2: {time: 0.4, value: 0.8}
      - 3: {time: 0.6, value: 0.7}
      - 4: {time: 0.8, value: 0.6}
      - 5: {time: 1.0, value: 0.5}
  m_AnimatorBindings:
    loopTime: 0      # Play once
    frameRate: 60
    wrapMode: 1      # Once